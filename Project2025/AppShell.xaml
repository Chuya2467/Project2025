<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Project2025.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Project2025"
    xmlns:pages="clr-namespace:Project2025.Pages"
    Title="Project2025">

    <ShellContent
    Title="HomePage"
    Route="HomePage"
    ContentTemplate="{DataTemplate pages:HomePage}" />

    <ShellContent
    Title="Movies"
    Route="MovieList"
    ContentTemplate="{DataTemplate pages:MovieList}" />

    <ShellContent
    Title="AccountPage"
    Route="AccountPage"
    ContentTemplate="{DataTemplate pages:AccountPage}" />

</Shell>